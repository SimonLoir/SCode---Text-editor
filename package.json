{
  "name": "scode",
  "version": "0.7.0",
  "description": "A smart code editor",
  "main": "main.js",
  "repository": "https://github.com/SimonLoir/SCode-Text-editor",
  "scripts": {
    "test": "electron .",
    "win-64": "electron-packager . --arch=x64 --plateform=win32 --electron-version=1.7.9 --out=releases --icon=src/logo.ico --overwrite --ignore=releases/",
    "win-32": "electron-packager . --arch=ia32 --plateform=win32 --electron-version=1.7.9 --out=releases --icon=src/logo.ico --overwrite --ignore=releases/",
    "all": "electron-packager . --all --electron-version=1.7.9 --out=releases --icon=src/logo.ico --overwrite --ignore=releases/",
    "dist": "build"
  },
  "author": "Simon Loir",
  "license": "MIT",
  "dependencies": {
    "emmet": "^1.6.3",
    "js-beautify": "^1.6.14",
    "jslint": "^0.11.0",
    "marked": "^0.3.6",
    "node-pty": "^0.7.2",
    "simple-git": "^1.76.0",
    "xterm": "^2.9.2"
  },
  "devDependencies": {
    "esprima": "^4.0.0"
  }
}
